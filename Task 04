#include <iostream>
using namespace std;

void printArray(){
    int array1[6]={1,2,3,4,5,6};
    int array2[6]={1,2,3,4,5,0};
    int array3[6]={1,2,0,4,0,0};
    
    cout<<"Values in Array1 by row are\n";
    for(int i=0; i<6; i++){
    
        cout<<array1[i];
        if(i=3){
            cout<<endl;
        }
    }
}
int main() {
printArray();
    return 0;
}

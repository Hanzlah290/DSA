#include <iostream>
using namespace std;
int main() {
    int num[10];
    cout << "Please Enter 10 Numbers:";
    for(int i=0; i < 10; i++){
        cin>>num[i];
    }
    cout<<"Element  Value  Histogram"<<endl;
    for(int i=0; i<10; i++){
        cout<<num[i-1]<<"\t\t"<<num[i]<<"\t\t";
        for(int j=0; j<=i; j++){
            cout<<"*";
        }
        cout<<endl;
    }

    return 0;
}
